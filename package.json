{
  "name": "CS-Explorer",
  "version": "4.0.0",
  "description": "一个用于管理在线云存储的现代化桌面应用。",
  "main": "out/main/index.js",
  "author": "吉庆喆",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "node ./scripts/inject-version.cjs && electron-vite dev",
    "build": "node ./scripts/inject-version.cjs && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "release:win": "npm run build && electron-builder --win -p always",
    "release:mac": "npm run build && electron-builder --mac -p always",
    "release:linux": "npm run build && electron-builder --linux -p always"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.569.0",
    "@aws-sdk/lib-storage": "^3.583.0",
    "@aws-sdk/s3-request-presigner": "^3.842.0",
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "ali-oss": "^6.23.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "electron-log": "^5.4.1",
    "electron-serve": "^2.1.1",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.1.8",
    "lucide-react": "^0.525.0",
    "proxy-agent": "^5.0.0",
    "react-router-dom": "^7.6.3",
    "react-syntax-highlighter": "^15.6.1",
    "sonner": "^2.0.6",
    "uuid": "^11.1.0",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/react": "^18.2.79",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "dotenv": "^17.0.1",
    "electron": "^37.2.4",
    "electron-builder": "^24.13.3",
    "electron-reloader": "^1.2.3",
    "electron-vite": "^2.1.0",
    "postcss": "^8.4.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "vite": "^5.2.10"
  },
  "build": {
    "appId": "com.r2.explorer",
    "productName": "CS-Explorer",
    "directories": {
      "output": "release"
    },
    "artifactName": "${productName}-Setup-${version}.${ext}",
    "files": [
      "out/**"
    ],
    "win": {
      "icon": "resources/icon.ico",
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": false,
      "license": "EULA.txt",
      "installerIcon": "resources/icon.ico"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "JiQingzhe2004",
        "repo": "R2APP"
      }
    ]
  }
}
